{
  "dashboard": {
    "title": "GitLab Overview",
    "tags": ["gitlab"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "GitLab Status",
        "type": "stat",
        "targets": [
          {
            "expr": "up{job=\"gitlab\"}",
            "legendFormat": "GitLab"
          }
        ],
        "gridPos": {"h": 4, "w": 4, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "HTTP Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(gitlab_transaction_duration_seconds_count[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4}
      },
      {
        "id": 3,
        "title": "GitLab Memory Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "process_resident_memory_bytes{job=\"gitlab\"}",
            "legendFormat": "Memory"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4}
      },
      {
        "id": 4,
        "title": "CI/CD Jobs",
        "type": "graph",
        "targets": [
          {
            "expr": "gitlab_runner_jobs",
            "legendFormat": "{{status}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12}
      }
    ]
  }
}


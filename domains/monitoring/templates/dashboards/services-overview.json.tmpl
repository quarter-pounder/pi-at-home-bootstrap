    {
      "type": "stat",
      "title": "Attention Required",
      "id": 101,
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum(1 - clamp_max(max_over_time(up{job=~\"forgejo|woodpecker|prometheus|loki|pihole\"}[5m]), 1))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "mappings": [
            {
              "type": "value",
              "options": {
                "0": {
                  "text": "Everything is in its right place",
                  "color": "#5fb3b3"
                }
              }
            },
            {
              "type": "range",
              "options": {
                "from": 1,
                "to": 100,
                "result": {
                  "text": "Attention required",
                  "color": "#d66060"
                }
              }
            }
          ]
        }
      },
      "options": { "orientation": "horizontal", "textMode": "value" }
    },
    {
      "type": "stat",
      "title": "Active Alerts",
      "id": 102,
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 1 },
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
